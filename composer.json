{
    "name": "lewislarsen/aether",
    "description": "Adds account settings to the laravel breeze package.",
    "keywords": ["laravel", "breeze", "account", "settings"],
    "license": "MIT",
    "support": {},
    "authors": [
        {
            "name": "Lewis Larsen",
            "email": "lewis.larsen@outlook.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "intervention/image": "^2.5",
        "laravel/breeze": "*",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LewisLarsen\\Aether\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LewisLarsen\\Aether\\AetherServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}